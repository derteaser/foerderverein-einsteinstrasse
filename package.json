{
  "name": "fv-einsteinstrasse-tailwind-theme",
  "version": "1.0.0",
  "author": "Jens Kuesters",
  "license": "MIT",
  "scripts": {
    "vite": "vite",
    "dev": "shx touch src/.lock && concurrently \"npm:server\" \"npm:vite\"",
    "server": "php -S localhost:8888 -t public server.php",
    "build": "shx rm -f src/.lock && vite build",
    "cert": "rm -rf .cert && mkdir -p .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem 'localhost'",
    "icons": "node icons.js"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.4",
    "bigpicture": "^2.6.1",
    "concurrently": "^7.0.0",
    "heroicons": "^1.0.6",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "shx": "^0.3.4",
    "simple-icons": "^6.15.0",
    "svg-mixer": "^2.3.14",
    "tailwindcss": "^3.0.23",
    "tailwindcss-debug-screens": "^2.2.1",
    "vite": "^2.9.0",
    "vite-plugin-live-reload": "^2.1.0"
  }
}
